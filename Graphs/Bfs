
#include <bits/stdc++.h>
#include<iostream>
using namespace std;

void bfs(int V, vector<int>&adj[], ){
    vector<int> bfs;
    
    queue<int> q;
    q.push(V);
    vis[0] = 1;
    
    
}

int main() {
	int v , e;
	cin >> v  >> e;
	vector<int> adj[v+1]; //vector of array to store edges
	vector<int> vis(v+1 , 0);
	
	for(int i = 0 ; i<e ; i++){
	    int u,v;
	    cin >> u >>v;
	    
	    adj[u].push_back(v);
	    adj[v].push_back(u);
	}

        bfs(0, adj, vis);
   
	
	return 0;
}
